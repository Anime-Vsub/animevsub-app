  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.vue ./",
    "pretit": "prettier \"**/*.{ts,vue,scss,html,md,json}\" --ignore-path .gitignore,.prettierignore",
    "format": "prettier --write \"**/*.{ts,vue,scss,html,md,json}\" --ignore-path .gitignore,.prettierignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:watch": "vitest",
    "test": "vitest run",
    "typing": "vue-tsc --noEmit",
    "dev:android": "pnpm dev -m capacitor -T android",
    "build:android": "pnpm build -m capacitor -T android",
    "quasar": "quasar",
    "icongenie": "icongenie generate -m capacitor -i meta/app_icon.png -b meta/app_bg@1024x500-blured.png --png-color 000 --splashscreen-color 000   --skip-trim",
    "gen-md-screenshot": "tsx ./scripts/gen-md-screenshot.ts",
    "release": "tsx scripts/release.ts",
    "ci": "pnpm format && pnpm lint --fix && pnpm typing"
  },
